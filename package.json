    "@types/jest": "24.0.11",
    "@types/node": "11.11.3",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "fast-memoize": "^2.5.1",
    "fastify": "^2.15.1",
    "fastify-cors": "^2.1.2",
    "gh-pages": "^2.0.1",
    "moment": "^2.24.0",
    "mongodb": "^3.1.13",
    "mui-datatables": "^2.0.0-beta.58",
    "node-fetch": "^2.3.0",
    "node-sass": "^4.11.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-moment": "^0.8.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "2.1.8",
    "tsc-watch": "^2.1.2",
    "tslint": "^5.14.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -b build -m \"Automated build\"",
    "server:build": "tsc -p server",
    "server:watch": "tsc-watch -p server --onSuccess \"yarn server:start\"",
    "server:start": "node dist/index.js",
    "server:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "server:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build -d"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/lowdb": "^1.0.7",
    "@types/mongodb": "^3.1.22",
    "@types/mui-datatables": "^2.0.6",
    "@types/node-fetch": "^2.1.6",
    "@types/react-router-dom": "^4.3.1",
    "tslint-config-standard": "^8.0.1"
  }
